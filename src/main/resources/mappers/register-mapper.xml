<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.config.dao.RegisterDAO">

    <!-- 아이디 중복확인 -->
    <select id="getOverlapId" parameterType="string" resultType="string">
        SELECT
            USER_ID
        FROM
            USER
        WHERE
            USER_ID = #{usrId}
    </select>

    <!-- 회원가입 -->
    <insert id="submitInnerUser" parameterType="InnerUserVo">
        INSERT INTO
            USER(USER_ID, USER_NM, PASSWD, PHONE_NO, EMAIL)
        VALUES(#{usrId},#{usrnm},#{passwd},#{phonno},#{email})
    </insert>

</mapper>